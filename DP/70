class Solution {
    int[] memo;
    public int climbStairs(int n) {
        memo=new int[n+1];
        dp(n);  
        return memo[n];
    }
    public int dp(int i){
        if(i<=2){
            return memo[i]=i;
        }
        if(memo[i]>0){
            return memo[i];
        }
        memo[i]=dp(i-1)+dp(i-2);
        return memo[i];
    }
}

